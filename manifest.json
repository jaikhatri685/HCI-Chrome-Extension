{
	"manifest_version": 2,
	"name": "HCI Project",
	"version": "1.0.1",
	"web_accessible_resources": ["*.js"],
	"description": "",
	"browser_action": {
		"default_popup": "popup.html"
	},
	"icons": {
		"32": "icon.png",
		"48": "icon.png",
		"64": "icon.png",
		"128": "icon.png"
	},
	
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"permissions": ["tabs","activeTab","storage", "notifications", "https://*/*", "http://*/*"]
}
